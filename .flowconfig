[ignore]
.*/node_modules/*

[include]

[libs]
flow-typed

[options]
emoji=true
unsafe.enable_getters_and_setters=true
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
module.file_ext=.less
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.name_mapper='^src/\(.+\)' -> '<PROJECT_ROOT>/src/\1'

[version]
^0.47.0
